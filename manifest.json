{
  "manifest_version": 3,
  "name": "KM Matrix Automation Tool",
  "version": "1.2",
  "description": "Advanced automation tool for Lam Research KM Matrix system - Download PDFs, STP files, and extract BOM structures with intelligent folder organization",
  "permissions": [
    "downloads",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://kmmatrix.fremont.lamrc.net/*"

  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "KM Matrix Automation - Lam Research",
    "default_icon": {
      "16": "data:image/svg+xml;base64,",
      "32": "data:image/svg+xml;base64,"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://kmmatrix.fremont.lamrc.net/BOMFinder*"],
      "js": ["xlsx.full.min.js", "contentScript.js"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["xlsx.full.min.js"],
      "matches": ["https://kmmatrix.fremont.lamrc.net/*"]
    }
  ],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  "author": "Lam Research Proto4Lab Team",
  "homepage_url": "https://lamresearch.com",
  "minimum_chrome_version": "88",
  "offline_enabled": false,
  "short_name": "KM Matrix Auto",
  "commands": {
    "start-pdf-download": {
      "suggested_key": {
        "default": "Ctrl+Shift+D"
      },
      "description": "Start PDF download operation"
    },
    "start-bom-extraction": {
      "suggested_key": {
        "default": "Ctrl+Shift+B"
      },
      "description": "Start BOM extraction and download"
    }
  }
}